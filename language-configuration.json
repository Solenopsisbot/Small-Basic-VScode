{
    "comments": {
        "lineComment": "'"
    },
    "brackets": [
        ["[", "]"],
        ["(", ")"]
    ],
    "wordPattern": "\\b[a-zA-Z][a-zA-Z0-9_]*\\b",
    "autoClosingPairs": [
        { "open": "(", "close": ")" },
        { "open": "[", "close": "]" },
        { "open": "\"", "close": "\"" }
    ]
}
